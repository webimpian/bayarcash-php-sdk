{
    "name": "webimpian/bayarcash-php-sdk",
    "description": "Bayarcash payment gateway PHP Sdk.",
    "version": "2.1.0",
    "type": "sdk",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "psr/simple-cache": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webimpian\\BayarcashSdk\\BayarcashSdkServiceProvider"
            ],
            "alias": {
                "BayarcashSdk": "Webimpian\\BayarcashSdk\\Facades\\Bayarcash"
            }
        }
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Webimpian\\BayarcashSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "faiz",
            "email": "faizafzal@webmpian.com"
        }
    ],
    "minimum-stability": "dev"
}
